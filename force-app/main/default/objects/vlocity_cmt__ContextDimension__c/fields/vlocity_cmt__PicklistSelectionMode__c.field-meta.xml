    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Single</fullName>
                <default>false</default>
                <label>Single</label>
            </value>
            <value>
                <fullName>Multiple</fullName>
                <default>false</default>
                <label>Multiple</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
